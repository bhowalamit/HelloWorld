    agent {windows
    }

    stages {
        stage('Build') {
            steps {
                echo '1. Building..'
            }
        }
        stage('Test') {
            steps {
                echo '2. Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo '3. Deploying....'
            }
        }
    }
}